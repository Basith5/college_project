// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Details {
  id       Int      @id @default(autoincrement())
  code     String
  reg      String   @unique
  class    String
  section  String
  exam     String
  results  results[]
}

model results {
  id       Int      @id @default(autoincrement())
  reg      String
  Q1       Int
  Q2       Int
  Q3       Int
  Q4       Int
  Q5       Int
  Q6       Int
  Q7       Int
  Q8       Int
  details  Details  @relation(fields: [reg], references: [reg])
}
