  Q6       Int
  Q7       Int
  Q8       Int
  Q9       Int
  Q10      Int
  Q11      Int
  Q12      Int
  Q13      Int
  Q14      Int
  Q15      Int
  Q16      Int
  Q17      Int
  Q18      Int
  Q19      Int
  Q20      Int
  Q21      Int
  Q22      Int
  Q23      Int
