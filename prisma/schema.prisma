generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model results {
  id       Int      @id @default(autoincrement())
  reg      String
  code     String
  class    String
  section  String
  exam     String
  Q1       Int
